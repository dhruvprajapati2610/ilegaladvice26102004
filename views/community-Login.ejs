    <script>
      const swiper = new Swiper(".swiper-container", {
        slidesPerView: 3,
        spaceBetween: 20,
        loop: true,
        grabCursor: true,

        pagination: {
          el: ".swiper-pagination",
          clickable: true,
          dynamicBullets: true,
        },
        breakpoints: {
