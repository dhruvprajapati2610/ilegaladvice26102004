      function showToast() {
        const toast = document.getElementById("toast");
        const toastMessage = document.getElementById("toastMessage");
        if (toast && toastMessage) {
          setTimeout(() => {
            toast.classList.add("hide");
            toastMessage.classList.add("hide");
            // toastMessage.innerText.style.display='none';
          }, 5000);
        }
      }

      window.onload = showToast;

      document.addEventListener("DOMContentLoaded", () => {
        const form = document.getElementById("login-form");
        form.addEventListener("submit", async (event) => {
          event.preventDefault();
          const formData = new FormData(form);
          const json = Object.fromEntries(formData.entries());
          try {
            const response = await fetch("/login", {
              method: "POST",
              headers: {
                "Content-Type": "application/json",
              },
              body: JSON.stringify(json),
            });
            const data = await response.json();
            Toastify({
              text: data.message,
              duration: 3000,
              close: true,
              gravity: "top",
              position: "center",
              backgroundColor: data.success ? "#4CAF50" : "FF0000",
            }).showToast();

            if (data.success) {
              setTimeout(() => {
                if (data.userType === "client") {
                  Toastify({
                    text: "Clients cannot access lawyer's community.",
                    duration: 3000,
                    close: true,
                    gravity: "top",
                    position: "center",
                    backgroundColor: "#FF0000",
                  }).showToast();
                  setTimeout(() => {
                    window.location.href = "/lawyerspage";
                  }, 1000);
                } else if (data.userType === "lawyer") {
                  window.location.href = "/community";
                }
              }, 650);
            }
          } catch (error) {
            // console.error('Error:', error);
            Toastify({
              text: "Invalid login credentials. please try again.",
              duration: 3000,
              close: true,
              gravity: "top",
              position: "center",
              backgroundColor: "#FF0000",
            }).showToast();
          }
        });
      });

